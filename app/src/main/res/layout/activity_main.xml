                            android:layout_height="45dp"
                            android:scaleType="centerCrop"/>
                        <TextView
                            android:background="@drawable/edge"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:paddingLeft="3dp"
                            android:text="OFF"
                            android:textSize="30dp"
