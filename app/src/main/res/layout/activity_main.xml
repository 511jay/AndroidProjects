                            android:id="@+id/jungle_spell2"
                            android:src="@drawable/question"
                            android:layout_width="45dp"
                            android:layout_height="45dp"
                            android:scaleType="centerCrop"/>
                        <TextView
                            android:background="@drawable/edge"
